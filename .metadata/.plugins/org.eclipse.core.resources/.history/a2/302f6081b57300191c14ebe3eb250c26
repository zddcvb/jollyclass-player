package com.jollyclass.airplayer.domain
{
	public class XsdTeacherBoxInfo extends DataInfo
	{
		/**
		 * 参数描述：定义在是否开通会员下的播放状态。这个参数只适用于教学端盒子。
		 */
		public var _teaching_status:String;
		
		/**
		 * 参数描述：教学端盒子的资源id(课件id或者素材id)。这个参数只适用于教学端盒子。
		 */
		public var _teaching_resource_id:String;
		
		/**
		 * 参数描述：定义在未开通会员下播放体验时长，以秒为单位。这个参数只适用于教学端盒子。
		 */
		public var _teaching_play_trial_duation:String;
		/**
		 * 参数描述：宿主应用app的包名，这个参数适用于所有产品。
		 */
		
		public var _package_name:String;
		
		public function XsdTeacherBoxInfo()
		{
			super();
		}

		public function index(){
			
		}
		
		public function get teaching_status():String
		{
			return _teaching_status;
		}

		public function set teaching_status(value:String):void
		{
			_teaching_status = value;
		}

		public function get teaching_resource_id():String
		{
			return _teaching_resource_id;
		}

		public function set teaching_resource_id(value:String):void
		{
			_teaching_resource_id = value;
		}

		public function get teaching_play_trial_duation():String
		{
			return _teaching_play_trial_duation;
		}

		public function set teaching_play_trial_duation(value:String):void
		{
			_teaching_play_trial_duation = value;
		}

		public function get package_name():String
		{
			return _package_name;
		}

		public function set package_name(value:String):void
		{
			_package_name = value;
		}

		override
		public function toString():String
		{
			return super.toString()+"XsdTeacherBoxInfo[teaching_status:"+_teaching_status+",teaching_resource_id:"+_teaching_resource_id+"_teaching_play_trial_duation"+_teaching_play_trial_duation+"package_name:"+_package_name+"]";
		}
	}
}