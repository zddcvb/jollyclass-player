package com.jollyclass.airplayer.util
{
	public class PathUtil
	{
		public function PathUtil()
		{
		}
		public static function selectPath(type:String,args:Array,paths:Array):String
		{
			var str:String;
			switch(type){
				for(var key in args){
					case key:
						break;
				}
				default:
					str=paths[paths-1];
					break;
			}
			return str;
		}
	}
}